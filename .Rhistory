viz_lm <-
function(df, dv, iv, year) {
dat <- df[df[["year"]] == year, ]
y <- log(dat[[dv]])
x <- log(dat[[iv]])
fit <- lm(y ~ x)
plot(y ~ x, main = year,
xlab = iv, ylab = dv)
lines(x, predict(fit), col = 'blue')
}
